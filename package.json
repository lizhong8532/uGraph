{
  "name": "ugraph",
  "title": "uGraph",
  "version": "1.0.0",
  "description": "A feature-rich SVG library",
  "main": "index.js",
  "scripts": {
    "dev&server": "concurrently --kill-others \"tsc\" \"npm run dev\" \"lite-server\"",
    "tsc": "tsc -w",
    "rollup": "rollup -w -c build/rollup.dev.config.js",
    "rollup_c": "rollup -c build/rollup.dev.config.js",
    "dev": "concurrently --kill-others \"npm run rollup\" \"lite-server\"",
    "dev:test": "karma start build/karma.dev.config.js",
    "dev:coverage": "karma start build/karma.coverage.config.js",
    "test:unit": "karma start build/karma.unit.config.js",
    "lint": "eslint src build test",
    "build": "rollup -c build/rollup.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lizhong8532/ugraph.git"
  },
  "keywords": [
    "ugraph",
    "svg",
    "javascript",
    "library"
  ],
  "author": "Jong Lee <mail@lizhong.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lizhong8532/ugraph/issues"
  },
  "homepage": "https://github.com/lizhong8532/ugraph#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-runtime": "^6.20.0",
    "buble": "^0.14.3",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.11.1",
    "eslint-plugin-mocha": "^4.7.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "phantomjs": "^2.1.7",
    "rollup": "^0.40.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-json": "^2.0.2",
    "rollup-watch": "^2.5.0",
    "webpack": "^1.14.0",
    "webpack-core": "^0.6.9"
  }
}
